class Solution {
public:
    bool isPalindrome(string s) {
  string s1,s2;    
        for(int i=0;s[i];i++)
      {
if ((s[i]>= 65 && s[i] <= 90)|| (s[i] >= 97 && s[i] <=122) || (s[i] >= 48 && s[i]<=57) )
              s1+=tolower(s[i]);
      }
        s2=s1;
        reverse(s1.begin(),s1.end());
        //cout<<s1<<" "<<s2;
        if(s1==s2)
            return true;
        return false;
    }
};
